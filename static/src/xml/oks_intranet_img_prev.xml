<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="oks_intranet.PreviewButton">
        <div class="oks_intranet_prev_cont">
            <button class="btn btn-primary" id="oks_intranet_prev_start" disabled="true">Ver archivos</button>
        </div>
    </t>

    <t t-name="oks_intranet.Preview">
        <div class="oks_item_per_row" id="oks_intranet_prev_widget">
            <div id="oks_intranet_prev_container">
                <div id="oks_intranet_prev_close_container">
                    <button id="oks_intranet_close_prev">+</button>
                </div>
                <div id="oks_intranet_prev_back_container">
                    <button id="oks_intranet_back_prev">&lt;</button>
                </div>
                <div id="oks_intranet_prev_display">
                    <p id="oks_intranet_prev_name"></p>
                    <img id="oks_intranet_prev_img"/>
                    <iframe id="oks_intranet_prev_pdf"></iframe>
                </div>
                <div id="oks_intranet_prev_next_container">
                    <button id="oks_intranet_next_prev">&gt;</button>
                </div>
            </div>
        </div>
    </t>
</templates>